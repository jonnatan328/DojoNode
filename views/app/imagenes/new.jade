extends ../../layout.jade
block contenido
	div(class="col-md-8 remove-float center-block big-top-space")
		h1 Subir nueva imagen
		form(action="/app/imagenes", method="POST" class="form-group")
			div
				label(for="title") Titulo
				input(type="text", name="title", placeholder="Titulo", id="title" class="form-control")
			div
				label(for="archivo") Imagen
				input(type="file", name="archivo",  id="title" class="form-control")
			div(class="row top-space")
				div(class="col-xs-12 col-sm-6")
					input(type="submit", value="Guardar" class="btn btn-info")
				